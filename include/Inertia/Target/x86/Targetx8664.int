target = x86_64
endian = little

regclass [GPR64 GPR32 GPR16 GPR8]

register rax{
    class = GPR64
    width = 64
}

register eax{
    class = GPR32
    width = 32
    parent rax
}

register ax{
    class = GPR16
    width = 16
    parent eax
}

register al{
    class = GPR8
    width = 8
    parent ax
}